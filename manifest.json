{
  "name": "Nikki's Info Extended",
  "version": "1.0",
  "description": "An extension that provide additional functionality for the LoveNikki fansite https://ln.nikkis.info/",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["*://*.nikkis.info/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
